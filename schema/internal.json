{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "first_name", "last_name", "email", "status"],
  "properties": {
    "id": { "type": "string" },
    "first_name": { "type": "string" },
    "last_name": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "status": {
      "type": "string",
      "enum": ["Active", "Inactive"]
    }
  }
}